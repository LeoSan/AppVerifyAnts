{
  "name": "with-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && next dev",
    "build": "npm run build:css && next build",
    "start": " npm run build:css && next start",
    "build:css": "postcss public/assets/tailwind.css -o public/assets/main.css",
    "watch:css": "postcss public/assets/tailwind.css -o public/assets/main.css -w"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "axios": "^0.21.1",
    "babel-plugin-macros": "^3.1.0",
    "formik": "^2.2.9",
    "moment": "^2.29.1",
    "next": "^11.1.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-google-recaptcha": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "sweetalert2": "^11.1.2",
    "swiper": "^6.8.0",
    "swiper-new": "^6.7.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@tailwindcss/jit": "^0.1.10",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.4"
  }
}
